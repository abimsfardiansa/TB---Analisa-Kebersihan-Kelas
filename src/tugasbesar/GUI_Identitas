    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        Identitas_kelas a=new Identitas_kelas(null, null, null, null);
        a.setNamaruang(String.valueOf(namaruang.getText()));
        a.setLokasi(String.valueOf(lokasi.getText()));
        a.setJurusan(String.valueOf(jurusan.getText()));
        a.setFakultas(String.valueOf(fakultas.getText()));
        JOptionPane.showMessageDialog(null, "APAKAH ANDA AKAN MENYIMPAN ?","",JOptionPane.INFORMATION_MESSAGE);
       // a.keluaransave();
        a.outdata();
        
    }                                        

