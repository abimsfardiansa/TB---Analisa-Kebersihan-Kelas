    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        analisis_kebersihan x=new analisis_kebersihan(null, 1, 1, 1);
        analisiskebersihan a=new analisiskebersihan();
        x.setSirkulasi(String.valueOf(sirkulasi.getText()));
        x.setCahaya(Integer.valueOf(pencahayaan.getText()));
        x.setKelembaban(Integer.valueOf(kelembaban.getText()));
        x.setSuhu(Integer.valueOf(suhu.getText()));
        
        sirkulasi3.setText(String.valueOf(a.analisissirkulasi(x.getSirkulasi())));
        pencahayaan3.setText(String.valueOf(a.analisiscahaya(x.getCahaya())));
        kelembaban3.setText(String.valueOf(a.analisislembab(x.getKelembaban())));
        suhu3.setText(String.valueOf(a.analisissuhu(x.getSuhu())));
        JOptionPane.showMessageDialog(null, "APAKAH ANDA AKAN MENYIMPAN ?","",JOptionPane.INFORMATION_MESSAGE);
        x.mlebu();
        x.keluaransave();
        
        
    }                                        

    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {                                         
