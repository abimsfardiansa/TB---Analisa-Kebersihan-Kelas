    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        analisis_kondisi1 a=new analisis_kondisi1(1, 1, 1, 1, 1, 1);
        analisiskondisi x=new analisiskondisi();
        a.setPanjang(Integer.valueOf(panjang.getText()));
        a.setLebar(Integer.valueOf(lebar.getText()));
        a.setKursi(Integer.valueOf(kursi.getText()));
        a.setPintu(Integer.valueOf(pintu.getText()));
        a.setJendela(Integer.valueOf(jendela.getText()));
        JOptionPane.showMessageDialog(null, "APAKAH ANDA AKAN MENYIMPAN ?","",JOptionPane.INFORMATION_MESSAGE);
        a.masukan();
        analisis1.setText(String.valueOf(x.jmlpintu(a.getPintu())));
        analisis2.setText(String.valueOf(x.jmljendela(a.getJendela())));
        int luasss=a.getPanjang()*a.getLebar();
        LUAS.setText(Integer.toString(luasss));
        bentukruang.setText(String.valueOf(x.Luas(a.getPanjang(),a.getLebar())));
    }                                        

