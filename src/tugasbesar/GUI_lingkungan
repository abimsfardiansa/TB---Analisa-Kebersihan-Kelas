    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        analisis_lingkungan x=new analisis_lingkungan(null, null, null, null, null);
        analisislingkungan a=new analisislingkungan();
        x.setLantai(String.valueOf(lantai.getText()));
        x.setDinding(String.valueOf(dinding.getText()));
        x.setAtap(String.valueOf(atap.getText()));
        x.setPintu(String.valueOf(pintu.getText()));
        x.setJendela(String.valueOf(jendela.getText()));
        JOptionPane.showMessageDialog(null, "APAKAH ANDA AKAN MENYIMPAN ?","",JOptionPane.INFORMATION_MESSAGE);
        x.inputling();
        //JML1.setText(String.valueOf(a.AnalisaJumlahSteker(x.getJumlah1())));
        lantai2.setText(String.valueOf(a.analantai(x.getLantai())));
        dinding2.setText(String.valueOf(a.anadinding(x.getDinding())));
        atap2.setText(String.valueOf(a.anaatap(x.getAtap())));
        pintu2.setText(String.valueOf(a.anapintu(x.getPintu())));
        jendela2.setText(String.valueOf(a.anajendela(x.getJendela())));

    }                                        

