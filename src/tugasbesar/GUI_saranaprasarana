    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
       Analisa_saranaprasarana x=new Analisa_saranaprasarana(1, null, null, 2, null, null, 3, null, null, 4, null, null, 5, null, null, 6, null, null, null, 7, null);
        Analisissaranaprasarana a=new Analisissaranaprasarana();
        x.setJumlah1(Integer.valueOf(jumlahsteker.getText()));
        x.setKondisi1(String.valueOf(kondisisteker.getText()));
        x.setPosisi1(String.valueOf(posisisteker.getText()));
        
        x.setJumlah2(Integer.valueOf(jumlahlcd.getText()));
        x.setKondisi2(String.valueOf(kondisilcd.getText()));
        x.setPosisi2(String.valueOf(posisilcd.getText()));
        
        x.setJumlah3(Integer.valueOf(jumlahlampu.getText()));
        x.setKondisi3(String.valueOf(kondisilampu.getText()));
        x.setPosisi3(String.valueOf(posisilampu.getText()));
        
        x.setJumlah4(Integer.valueOf(jumlahkipas.getText()));
        x.setKondisi4(String.valueOf(kondisikipas.getText()));
        x.setPosisi4(String.valueOf(posisikipas.getText()));
        
        x.setJumlah5(Integer.valueOf(jumlahac.getText()));
        x.setKondisi5(String.valueOf(kondisiac.getText()));
        x.setPosisi5(String.valueOf(posisiac.getText()));
        
        x.setSsid(String.valueOf(ssid.getText()));
        x.setBandwidth(Integer.valueOf(bandwidth.getText()));
        x.setLogin(String.valueOf(login.getText()));
        
        x.setJumlah6(Integer.valueOf(jumlahcctv.getText()));
        x.setKondisi6(String.valueOf(kondisicctv.getText()));
        x.setPosisi6(String.valueOf(posisicctv.getText()));
        x.setPosisi6a(String.valueOf(posisicctv1.getText()));
        
        JOptionPane.showMessageDialog(null, "APAKAH ANDA AKAN MENYIMPAN ?","",JOptionPane.INFORMATION_MESSAGE);
        x.input();
        x.keluaransave();
        jumlahhsteker.setText(String.valueOf(a.AnalisaJumlahSteker(x.getJumlah1())));
        kondisiisteker.setText(String.valueOf(a.AnalisaKondisiSteker(x.getKondisi1())));
        posisiisteker.setText(String.valueOf(a.AnalisaPosisiSteker(x.getPosisi1())));
        
        jumlahhlcd.setText(String.valueOf(a.AnalisaJumlahLcd(x.getJumlah2())));
        kondisiilcd.setText(String.valueOf(a.AnalisaKondisiLcd(x.getKondisi2())));
        posisiilcd.setText(String.valueOf(a.AnalisaPosisiLcd(x.getPosisi2())));
        
        jumlahhlampu.setText(String.valueOf(a.AnalisaJumlahLampu(x.getJumlah3())));
        kondisiilampu.setText(String.valueOf(a.AnalisaKondisiLampu(x.getKondisi3())));
        posisiilampu.setText(String.valueOf(a.AnalisaPosisiLampu(x.getPosisi3())));
        
        jumlahhkipas.setText(String.valueOf(a.AnalisaJumlahKipas(x.getJumlah4())));
        kondisiikipas.setText(String.valueOf(a.AnalisaKondisiKipas(x.getKondisi4())));
        posisiikipas.setText(String.valueOf(a.AnalisaPosisiKipas(x.getPosisi4())));
        
        jumlahhac.setText(String.valueOf(a.AnalisaJumlahac(x.getJumlah5())));
        kondisiiac.setText(String.valueOf(a.AnalisaKondisiac(x.getKondisi5())));
        posisiiac.setText(String.valueOf(a.AnalisaPosisiac(x.getPosisi5())));
        
        ssidd.setText(String.valueOf(a.pilihssid(x.getSsid())));
        loginn.setText(String.valueOf(a.AnalisaLogin(x.getLogin())));
        
        jumlahhcctv.setText(String.valueOf(a.AnalisaJumlahcctv(x.getJumlah6())));
        kondisiicctv.setText(String.valueOf(a.AnalisaKondisicctv(x.getKondisi6())));
        posisiicctv.setText(String.valueOf(a.AnalisaPosisicctv1(x.getPosisi6())));
        posisiicctv1.setText(String.valueOf(a.AnalisaPosisicctv1(x.getPosisi6a())));
    }                                        

