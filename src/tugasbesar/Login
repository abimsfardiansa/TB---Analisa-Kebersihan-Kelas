    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        String user,pass;
        user=username.getText();
        pass=password.getText();
        if (user.equalsIgnoreCase("abims")&& (pass.equalsIgnoreCase("088")) ||
            (user.equalsIgnoreCase("danang")) && (pass.equalsIgnoreCase("086")) ||
            (user.equalsIgnoreCase("rizky")) && (pass.equalsIgnoreCase("082")))
            
        {
            JOptionPane.showMessageDialog(null, "Selamat \n Anda Berhasil Login","LOGIN SUKSES",JOptionPane.INFORMATION_MESSAGE);
            GUI_identitaskelas z=new GUI_identitaskelas();
            z.identitaskelas();
            setVisible(false);
        //    new inina().setVisible(true);
        }
        else
        {
            JOptionPane.showMessageDialog(null, "Maaf Silahkan periksa Username dan Password","LOGIN GAGAL",JOptionPane.INFORMATION_MESSAGE);
        }
    }                                        

